<%= @ship_address.to_s %>
<label>Primary Contact:</label><br>
<label>First Name:
  <input class="customer-form-input" type='text' name="spree_address[firstname]" value="<%= @ship_address.firstname %>">
</label>
<label>Last Name:
  <input class="customer-form-input" type='text' name="" value="<%= @ship_address.lastname %>">
</label>
<br>

<label>Primary Phone:
  <input class="customer-form-input" type="text" name="" value="<%= @ship_address.phone %>">
</label>
<br>

<label>Customer Address: </label>
<br>

<label>Address Line 1:
  <input class="customer-form-input" type="text" name="" value="<%= @ship_address.address1 %>">
</label>
<br>

<label>Address Line 2:
  <input class="customer-form-input" type="text" name="" value="<%= @ship_address.address2 %>">
</label>
<br>

<label>City:
  <input class="customer-form-input" type="text" name="" value="<%= @ship_address.city %>">
</label>
<br>

<label>State:
  <select class="select-state" name="" value="">
    <% @states.each do |state| %>
      <option><%= state.name %></option>
    <% end %>
  </select>
</label>
<label>Zip Code:
  <input class="customer-form-input zipcode" type="text" name="" value="<%= @ship_address.zipcode %>">
</label>
<br>

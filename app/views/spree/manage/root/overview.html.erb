<body>
	<div class="sweet_app wrapper">
		<div class="app_ui-wrapper">
			<header>
				<div class="logo"></div>
				<div class="header-nav">
					<div class="nav-ico"></div>
					<div id="sb-search" class="search-ico sb-search">
						<form>
							<span class="sb-icon-search"><i class="fa fa-search sb-icon-search"></i> </span>
							<input class="sb-search-input" placeholder="Enter your search term..." type="text" value="" name="search" id="search">
							<input class="sb-search-submit" type="submit" value="">
						</form>
					</div>
					<div class="exit-ico"><i class="fa fa-sign-out"></i></div>
					<div class="settings-ico"><i class="fa fa-cog"></i></div>
				</div>
			</header>
			<div class="relative">
				<aside class="active active-tablet">
					<div class="app-user-block">
						<div class="photo online">
							<!-- <img src="images/spree/manage/img/user-photo.jpg" alt=""> -->
							<%=image_tag("spree/manage/img/user-photo.jpg")%>
						</div>
						<div class="info tablet-hide">
							<p class="user-name">Djone Doe</p>
							<p class="user-role">Manager</p>
						</div>
						<div class="clear"></div>
					</div>
					<div class="nav-block" id="accordion">
						<h3><%=image_tag("spree/manage/img/nav-ico1.png")%>
							<!-- <img src="images/spree/manage/img/nav-ico1.png" alt="">  -->

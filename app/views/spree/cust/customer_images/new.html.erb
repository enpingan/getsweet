<%#= render partial: 'spree/admin/shared/error_messages', locals: { target: @vendor } %>

<% content_for :page_title do %>
  <%= Spree.t(:new_image) %>
<% end %>

<%= form_for [:cust, @account, @account_image], url: account_account_images_url, html: { multipart: true } do |f| %>
  <fieldset data-hook="new_image">
    <%= render partial: 'form', locals: { f: f } %>

    <div class="form-actions" data-hook="buttons">
      <%= button Spree.t('actions.update'), 'save' %>
      <span class="or"><%= Spree.t(:or) %></span>
      <%= button_link_to Spree.t('actions.cancel'), spree.account_account_images_url, icon: 'delete', id: 'cancel_link' %>
    </div>
  </fieldset>
<% end %>

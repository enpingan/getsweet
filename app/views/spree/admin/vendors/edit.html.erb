<% content_for :page_title do %>
	<%# page title on _vendor_tabs partial %>
  <%#= Spree.t(:editing_resource, resource: Spree::Vendor.model_name.human) %>
<% end %>

<%= render partial: 'spree/admin/shared/vendor_tabs', locals: {current: :details} %>
<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @vendor } %>

<%= form_for [:admin, @vendor], method: :put, html: { multipart: true }  do |f| %>
 <fieldset>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <%= render :partial => 'spree/admin/shared/edit_resource_links' %>
 </fieldset>
<% end %>

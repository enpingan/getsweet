.app_ui-wrapper{
	position: relative;
}
.app_ui-wrapper aside{
	position: absolute;
	top: 0;
	left: 0;
	width: 0;
	height: 0;
	margin-left: -100%;
	overflow: hidden;
	float: left;
	display: block;
	z-index: 50;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transition: all 0.17s ease;
	-moz-transition: all 0.17s ease;
	-o-transition: all 0.17s ease;
	transition: all 0.17s ease;
}
.app_ui-wrapper aside::after {
    content: "";
    height: 2000px;
    bottom: 0;
    background: #4C4743 none repeat scroll 0% 0%;
    width: 100%;
    position: relative;
    left: 0;
    display: block;
}
.app_ui-wrapper aside.active{
	margin-left: 0;
	width: 270px;
	height: auto;
	background: #4C4743;
}
.app_ui-wrapper .app_ui-block{
	padding: 30px;
	width: 100%;
	background: #F9F5F1;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transition: all 0.2s ease;
	-moz-transition: all 0.2s ease;
	-o-transition: all 0.2s ease;
	transition: all 0.2s ease;
}
.app_ui-wrapper .app_ui-block form{
	margin-top: 13px;
}
.app_ui-wrapper .app_ui-block.active{
	padding: 30px 30px 30px 300px;
}
.app_ui-wrapper aside h3 {
    font-size: 13px;
    outline: none;
    cursor: pointer;
    line-height: 23px;
    padding: 17px 27px 17px 24px;
    color: #e4e0db;
    font-weight: 300;
    position: relative;
    background: #4C4743;
    border-left: 3px solid transparent;
}
.app_ui-wrapper aside h3 span{
	display: block;
}
.app_ui-wrapper aside h3:hover {
	background: #3B3734;
	border-left: 3px solid #ff7e00;
}
.app_ui-wrapper aside h3:after{
	content: "";
	display: block;
	clear: both;
}
.app_ui-wrapper aside h3.active{
	color: #ff7e00;
}
.app_ui-wrapper aside h3.active:hover{
	background: #4C4743;
    border-left: 3px solid #4C4743;
}
.app_ui-wrapper aside h3.active i{
	-webkit-transform: rotate(-90deg);
	-moz-transform: rotate(-90deg);
	-o-transform: rotate(-90deg);
	-ms-transform: rotate(-90deg);
	transform: rotate(-90deg);
}
.app_ui-wrapper aside h3 img {
    display: inline-block;
    float: left;
    margin: 0px 14px 0px 0;
    padding: 1px 0px 2px;
}
.app_ui-wrapper aside h3 i {
    font-size: 16px;
    line-height: 16px;
    margin-right: 4px;
    float: right;
    display: inline-block;
}
.app_ui-wrapper aside .nav-block ul {
    padding: 0 20px 8px 55px;
    background: #4C4743;
    margin-right: 30px;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.app_ui-wrapper aside .nav-block ul li:first-child{
	padding-top: 8px;
	border-top: 1px solid #6A645E;
}
.app_ui-wrapper aside .nav-block ul li {
    font-size: 13px;

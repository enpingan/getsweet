.header .exit-ico, .header .settings-ico{
    margin-left: 21px;
	float: right;
}
.app_ui-wrapper{
	position: relative;
}
.app_ui-wrapper aside{
	position: fixed;
	top: 52px;
	left: 0;
	width: 0;
	bottom: 0;
	// min-height: 100%;
	margin-left: -100%;
	overflow: hidden;
	float: left;
	display: block;
	z-index: 1000;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transition: all 0.17s ease;
	-moz-transition: all 0.17s ease;
	-o-transition: all 0.17s ease;
	transition: all 0.17s ease;
}
.app_ui-wrapper aside.active.active-tablet{
	overflow-y: auto;
}
.app_ui-wrapper aside.active.active-tablet::-webkit-scrollbar{
	background-color: #4C4743;
}
.app_ui-wrapper aside.active{
	margin-left: 0;
	width: 270px;
	height: auto;
	background: #4C4743;
}
.app_ui-wrapper .app_ui-block{
	padding: 30px;
	width: 100%;
	background: #F9F5F1;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	-webkit-transition: all 0.2s ease;
	-moz-transition: all 0.2s ease;
	-o-transition: all 0.2s ease;
	transition: all 0.2s ease;
}
.app_ui-wrapper .app_ui-block form{
	margin-top: 13px;
}
.app_ui-wrapper .app_ui-block.active{
	padding: 30px 30px 30px 300px;
}
.app_ui-wrapper aside h3 {
    font-size: 13px;
    outline: none;
    cursor: pointer;
    line-height: 23px;
    padding: 17px 27px 17px 24px;
    color: #e4e0db;
    font-weight: 300;
    position: relative;
    background: #4C4743;
    border-left: 3px solid transparent;
}
.app_ui-wrapper aside h3 span{
	display: block;
}
.app_ui-wrapper aside h3:hover {
	background: #3B3734;
	border-left: 3px solid #ff7e00;
}
.app_ui-wrapper aside h3:after{
	content: "";
	display: block;
	clear: both;
}
.app_ui-wrapper aside h3.active{
	color: #ff7e00;
}
.app_ui-wrapper aside h3.active:hover{
	background: #4C4743;
    border-left: 3px solid #4C4743;
}
.app_ui-wrapper aside h3.active i{
	-webkit-transform: rotate(-90deg);
	-moz-transform: rotate(-90deg);
	-o-transform: rotate(-90deg);
	-ms-transform: rotate(-90deg);
	transform: rotate(-90deg);
}
.app_ui-wrapper aside h3 img {
    display: inline-block;
    float: left;
    margin: 0px 14px 0px 0;
    padding: 1px 0px 2px;
}
.app_ui-wrapper aside h3 i {
    font-size: 16px;
    line-height: 16px;
    margin-right: 4px;
    float: right;
    display: inline-block;
}
.app_ui-wrapper aside .nav-block ul {
    padding: 0 20px 8px 55px;
    background: #4C4743;
    margin-right: 30px;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.app_ui-wrapper aside .nav-block ul li:first-child{
	padding-top: 8px;
	border-top: 1px solid #6A645E;
}
.app_ui-wrapper aside .nav-block ul li {
    font-size: 13px;
    line-height: 34px;
    font-weight: 500;
    color: #6e665f;
    list-style: none;
}
.app_ui-wrapper aside .nav-block ul li a{
	-webkit-transition: all 0.2s ease;
	-moz-transition: all 0.2s ease;
	-o-transition: all 0.2s ease;
	transition: all 0.2s ease;
	text-decoration: none;
	color: #b7b1ac;
}
.app_ui-wrapper aside .nav-block ul li a:hover{
	color: #FF7E00;
}
.app_ui-wrapper .app_ui-block:after{
	content: "";
	display: block;
	clear: none;
}
.app_ui-wrapper .app_ui-block .app-dash-widget {
    background: #FFF;
    padding: 21px 19px 18px 19px;
    border-left: 1px solid #F2EEEA;
    border-right: 1px solid #F2EEEA;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-box-shadow: 0px 3px 3px #E7E7E7;
    -moz-box-shadow: 0px 3px 3px #E7E7E7;
    box-shadow: 0px 3px 3px #E7E7E7;

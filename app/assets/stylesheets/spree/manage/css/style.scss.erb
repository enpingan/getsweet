}
.sweet_app .account-form p input[type="submit"]:hover, #basic-modal-content form input[type="submit"]:hover{
	background: #EF6E00;
}
.sweet_app .divider, #basic-modal-content .divider{
    height: 1px;
    width: 100%;
    margin: 19px 0;
    background: #DADADA;
    position: relative;
}
.sweet_app .divider:before, #basic-modal-content .divider:before{
	content: "";
	position: absolute;
	top: -4px;
	left: 2%;
	width: 96%;
	height: 1px;
	background: #DADADA;
}
.sweet_app .divider:after, #basic-modal-content .divider:after{
	content: "";
	position: absolute;
	bottom: -4px;
	left: 2%;
	width: 96%;
	height: 1px;
	background: #DADADA;
}


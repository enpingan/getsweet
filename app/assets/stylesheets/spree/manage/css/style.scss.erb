/*-------------------------------------------------------------------------*/
/*	1. Globally Applied Styles
/*-------------------------------------------------------------------------*/
@import url(<%= asset_path "fonts.css" %>);
html{
	height: 100%;
}
body{
	font-family: 'p22_underground_cy_probook';
	font-weight: 400;
	font-size: 14px;
	overflow-x: hidden;
	height: 100%;
	min-width: 320px;
	background-color:#E4E4E4;
	color: #6e665f;
}

*{
	margin: 0;
	padding: 0;
}
a{
	text-decoration: none;
}

label{
	text-align: left;
}

button {
	border: none;
}
img{
	max-width: 100%;
	height: auto;
}
.none {
	display: none;
}
.clear {
	clear: both;
}
.left {
	float: left;
}
.right {
	float: right;
}
.aligncenter {
	text-align: center;
}
.alignleft {
	text-align: left;
}
.alignright {
	text-align: right;
}
.relative{
	position: relative;
	min-height: 100%;
}
::-webkit-input-placeholder {color:#ccc;}
::-moz-placeholder          {color:#ccc;}
:-moz-placeholder           {color:#ccc;}
:-ms-input-placeholder      {color:#ccc;}
:focus::-webkit-input-placeholder {color: transparent}
:focus::-moz-placeholder          {color: transparent}
:focus:-moz-placeholder           {color: transparent}
:focus:-ms-input-placeholder      {color: transparent}
/*-------------------------------------------------------------------------*/
/*	2.	Content Styles
/*-------------------------------------------------------------------------*/

.noResults {
	display: none;
}

.liveFilterContainer a{
	text-decoration: none;
	color: #6e665f;
}
.liveFilterContainer a:hover{
	color: #ff7e00;
}

.app_ui-wrapper .app_ui-block .liveFilterContainer label{
	text-align: left;
	font-size: 16px;
}

.app_ui-wrapper .app_ui-block .liveFilterContainer input{
	font-size: 14px;
	padding: 5px 10px;
	margin-bottom: 20px;
}

.sweet_app{
	min-height: 100%;
	overflow: hidden;
}
.sweet_app.wrapper{
	max-width: 1440px;
	margin: 0 auto;
	background: #F9F5F1;
	-webkit-box-shadow: 0px 0px 10px rgba(0,0,0,0.4);
	-moz-box-shadow: 0px 0px 10px rgba(0,0,0,0.4);
	box-shadow: 0px 0px 10px rgba(0,0,0,0.4);
}
.sweet_app .welcome-block{
	display: table;
	height: 100%;
	width: 100%;
	text-align: center;
	padding: 50px 0;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: image_url("spree/manage/img/app-bg.jpg") center no-repeat;
	background-size: cover;
}
.sweet_app .welcome-block .welcome-logo-block, .sweet_app .account-form-block{
	display: table-cell;
	vertical-align: middle;
}
.sweet_app .welcome-logo-block .logo{
	margin: 0 auto 82px auto;
	width: 333px;
	height: 70px;
	background: image_url("spree/manage/img/logo1.png") center no-repeat;
	background-size: contain;
}
.sweet_app .account-block{
	padding: 20px 0;
	display: table;
	height: 100%;
	width: 100%;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.sweet_app .account-block .logo {
    position: absolute;
    top: 25px;
    left: 50%;
    margin-left: -118px;
    width: 222px;
    height: 48px;
    background: image_url("spree/manage/img/logo1.png") center no-repeat;
    background-size: contain;
}
.welcome-logo-block h1 {
    font-size: 50px;
    line-height: 50px;
    font-weight: 300;
    text-align: center;
    letter-spacing: 4px;
    color: #6e665f;
    margin: 0 0 38px 0;
}
.sweet_app .welcome-form {
    background: #6E665F;
    max-width: 700px;
    text-align: left;
    margin: 0 auto 50px auto;
    padding: 30px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.sweet_app .welcome-form input[type="text"], .sweet_app .welcome-form input[type="email"], .sweet_app .welcome-form input[type="password"]{
	width: 100%;
	display: block;
	height: 58px;
	font-size: 20px;
	border: 1px solid transparent;
	padding: 0 15px;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	-o-transition: all 0.3s ease;
	transition: all 0.3s ease;
}
.sweet_app .welcome-form input[type="text"]:focus, .sweet_app .welcome-form input[type="email"]:focus, .sweet_app .welcome-form input[type="password"]:focus{
	-webkit-box-shadow: inset 0px 0px 5px #808080;
	-moz-box-shadow: inset 0px 0px 5px #808080;
	box-shadow: inset 0px 0px 5px #808080;
	outline: none;
	border: 1px solid #808080;
}
.sweet_app .welcome-form p{
	margin: 0 0 30px 0;
}
.sweet_app .welcome-form p:last-child{
	margin: 0;
}
.sweet_app .welcome-form label {
    font-size: 30px;
    display: block;
    line-height: 30px;
    margin-bottom: 8px;
    color: #FFF;
}
.sweet_app .welcome-form input[type="submit"]{
	display: inline-block;
	height: 60px;
	padding: 0 75px;
	float: left;
	outline: none;
	border: none;
	background: #FF7E00;
	color: #FFF;
	font-size: 30px;
	text-transform: uppercase;
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	-o-transition: all 0.3s ease;
	transition: all 0.3s ease;
}
.sweet_app .welcome-form input[type="submit"]:hover{
	background: #EF6E00;
}
#pass-restore-link{
	display: inline-block;
	font-size: 30px;
	line-height: 30px;
	margin-top: 15px;
	color: #FFF;
	text-decoration: none;
	float: right;
	border-bottom: 2px solid #FFF;
}
.sweet_app .welcome-cta-link{
	font-family: 'p22_underground_cy_probold';
	display: inline-block;
	font-size: 50px;
	line-height: 50px;
	color: #FF7E00;
	text-decoration: none;
	border-bottom: 2px solid #FF7E00;
}
.sweet_app .account-block h1{
	font-family: 'p22_underground_cy_prodemi';
	font-size: 60px;
	line-height: 60px;
	color: #6e665f;
	margin: 70px 0 0 0;
	letter-spacing: 2px;
	text-align: center;
}
.sweet_app .account-form-block .inner{
	max-width: 640px;
	margin: 0 auto;
}
.sweet_app .account-form-block .account-form {
    margin-top: 54px;
}
.sweet_app .account-form p{
	margin-bottom: 20px;
}
.sweet_app .account-form p input {
    height: 60px;
    border: 1px solid #DADADA;
    padding: 0 20px;
    font-size: 24px;
    display: block;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    -webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	-o-transition: all 0.3s ease;
	transition: all 0.3s ease;
}
.sweet_app .account-form p input:focus{
	-webkit-box-shadow: inset 0px 0px 8px #ddd;
	-moz-box-shadow: inset 0px 0px 8px #ddd;
	box-shadow: inset 0px 0px 8px #ddd;
	outline: none;
}
.sweet_app .account-form p input[type="submit"], #basic-modal-content form input[type="submit"]{
    display: inline-block;
    width: auto;
    height: 55px;
    padding: 0 24px;
    border: none;
    font-size: 26px;
    color: #FFF;
    background: #FF7E00;
    line-height: 60px;
    margin-top: 29px;
    text-transform: uppercase;
    font-family: 'p22_underground_cy_prodemi';
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
    transition: all 0.3s ease;
}
.sweet_app .account-form p input[type="submit"]:hover, #basic-modal-content form input[type="submit"]:hover{
	background: #EF6E00;
}
.sweet_app .divider, #basic-modal-content .divider{
    height: 1px;
    width: 100%;
    margin: 19px 0;
    background: #DADADA;
    position: relative;
}
.sweet_app .divider:before, #basic-modal-content .divider:before{
	content: "";
	position: absolute;
	top: -4px;
	left: 2%;
	width: 96%;
	height: 1px;
	background: #DADADA;
}
.sweet_app .divider:after, #basic-modal-content .divider:after{
	content: "";
	position: absolute;
	bottom: -4px;
	left: 2%;
	width: 96%;
	height: 1px;
	background: #DADADA;
}

.header {
	// position: relative;
    background: #FFF;
    height: 52px;
    z-index: 30;
  //   -webkit-box-shadow: 0px 1px 8px #ccc;
	// -moz-box-shadow: 0px 1px 8px #ccc;

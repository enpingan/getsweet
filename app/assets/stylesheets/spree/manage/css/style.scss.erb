}
.sweet_app .welcome-form input[type="submit"]:hover{
	background: #EF6E00;
}
#pass-restore-link{
	display: inline-block;
	font-size: 30px;
	line-height: 30px;
	margin-top: 15px;
	color: #FFF;
	text-decoration: none;

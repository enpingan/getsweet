/*-------------------------------------------------------------------------*/
/*	1. Globally Applied Styles
/*-------------------------------------------------------------------------*/
@import url(<%= asset_path "fonts.css" %>);
html{
	height: 100%;
}
body{
	font-family: 'p22_underground_cy_probook';
	font-weight: 400;
	font-size: 14px;
	overflow-x: hidden;
	height: 100%;
	min-width: 320px;
	background-color:#E4E4E4;
	color: #6e665f;
}

*{
	margin: 0;
	padding: 0;
}
label{
	text-align: left;
}

button {
	border: none;
}
img{
	max-width: 100%;
	height: auto;
}
.none {
	display: none;
}
.clear {
	clear: both;
}
.left {
	float: left;
}
.right {
	float: right;
}
.aligncenter {
	text-align: center;
}
.alignleft {
	text-align: left;
}
.alignright {
	text-align: right;
}
.relative{
	position: relative;
}
::-webkit-input-placeholder {color:#ccc;}
::-moz-placeholder          {color:#ccc;}
:-moz-placeholder           {color:#ccc;}
:-ms-input-placeholder      {color:#ccc;}
:focus::-webkit-input-placeholder {color: transparent}
:focus::-moz-placeholder          {color: transparent}
:focus:-moz-placeholder           {color: transparent}
:focus:-ms-input-placeholder      {color: transparent}
/*-------------------------------------------------------------------------*/
/*	2.	Content Styles
/*-------------------------------------------------------------------------*/

.noResults {
	display: none;
}

.liveFilterContainer a{
	text-decoration: none;
	color: #6e665f;
}
.liveFilterContainer a:hover{
	color: #ff7e00;
}

.app_ui-wrapper .app_ui-block .liveFilterContainer label{
	text-align: left;
	font-size: 16px;
}

.app_ui-wrapper .app_ui-block .liveFilterContainer input{
	font-size: 14px;
	padding: 5px 10px;
	margin-bottom: 20px;
}

.sweet_app{
	min-height: 100%;
	overflow: hidden;
}
.sweet_app.wrapper{
	max-width: 1440px;
	margin: 0 auto;
	background: #F9F5F1;
	-webkit-box-shadow: 0px 0px 10px rgba(0,0,0,0.4);
	-moz-box-shadow: 0px 0px 10px rgba(0,0,0,0.4);
	box-shadow: 0px 0px 10px rgba(0,0,0,0.4);
}
.sweet_app .welcome-block{
	display: table;
	height: 100%;
	width: 100%;
	text-align: center;
	padding: 50px 0;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    background: image_url("spree/manage/img/app-bg.jpg") center no-repeat;
	background-size: cover;
}
.sweet_app .welcome-block .welcome-logo-block, .sweet_app .account-form-block{
	display: table-cell;
	vertical-align: middle;
}
.sweet_app .welcome-logo-block .logo{
	margin: 0 auto 82px auto;
	width: 333px;
	height: 70px;
	background: image_url("spree/manage/img/logo1.png") center no-repeat;
	background-size: contain;
}
.sweet_app .account-block{
	padding: 20px 0;
	display: table;
	height: 100%;
	width: 100%;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.sweet_app .account-block .logo {
    position: absolute;
    top: 25px;
    left: 50%;
    margin-left: -118px;
    width: 222px;
    height: 48px;
    background: image_url("spree/manage/img/logo1.png") center no-repeat;
    background-size: contain;
}
.welcome-logo-block h1 {
    font-size: 50px;
    line-height: 50px;
    font-weight: 300;
    text-align: center;
    letter-spacing: 4px;
    color: #6e665f;
    margin: 0 0 38px 0;
}
.sweet_app .welcome-form {
    background: #6E665F;
    max-width: 700px;
    text-align: left;
    margin: 0 auto 50px auto;
    padding: 30px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.sweet_app .welcome-form input[type="text"], .sweet_app .welcome-form input[type="email"], .sweet_app .welcome-form input[type="password"]{
	width: 100%;
	display: block;
	height: 58px;
	font-size: 20px;
